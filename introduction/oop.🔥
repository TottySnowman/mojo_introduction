struct Banana:
    var Rype:Bool
    var Length: Float32
    var Color:String

    fn __init__(inout self, Rype:Bool, Length:Float32, Color:String):
        self.Rype = Rype
        self.Length = Length
        self.Color = Color

    fn getRype(self) -> Bool:
        return self.Rype

    fn getLength(self) -> Float32:
        return self.Length
    
    fn getColor(self) -> String:
        return self.Color

fn main():
    var bana = Banana(True, 5.0, "Yellow")
    print(bana.getRype())
    print(bana.getLength())
    print(bana.getColor())
